<?xml version="1.0" encoding="UTF-8"?>
<block name="OpsCv" copy_type="delivery" src_path="" dst_path="" install_path="" install_own="$username:$usergroup" install_mod="550" install_type="run;devel">
    <dir_info install_mod="550">
        <path value="$(TARGET_ENV)/lib64"/>
    </dir_info>
    <file_info value="opapi_cv_lib" copy_type="delivery" src_path="build/"
        dst_path="built-in/op_impl/ai_core/tbe/op_api/lib/linux/$(ARCH)/"
        install_path="ops_cv/built-in/op_impl/ai_core/tbe/op_api/lib/linux/$(ARCH)/" install_mod="555">
        <file value="libopapi_cv.so"
            file_type="shared"
            install_softlink="$(TARGET_ENV)/lib64/libopapi_cv.so"
            pkg_inner_softlink="ops_cv:lib64/libopapi_cv.so"/>
    </file_info>
    <file_info value="opgraph_cv_lib" copy_type="delivery" src_path="build/"
        dst_path="built-in/op_graph/lib/linux/$(ARCH)/"
        install_path="ops_cv/built-in/op_graph/lib/linux/$(ARCH)/" install_mod="555">
        <file value="libopgraph_cv.so"
            file_type="shared"
            install_softlink="$(TARGET_ENV)/lib64/libopgraph_cv.so"/>
    </file_info>
    <file_info value="ophost_cv_lib" copy_type="delivery" src_path="build/"
        dst_path="built-in/op_impl/ai_core/tbe/op_tiling/lib/linux/$(ARCH)/"
        install_path="ops_cv/built-in/op_impl/ai_core/tbe/op_host/lib/linux/$(ARCH)/" install_mod="555">
        <file value="libophost_cv.so"
            file_type="shared"
            install_softlink="$(TARGET_ENV)/lib64/libophost_cv.so"/>
    </file_info>
    <file_info value="ops_common_cv_lib" copy_type="delivery" src_path="build/"
        dst_path="lib/"
        install_path="ops_cv/$(TARGET_ENV)/lib64" install_mod="555">
        <file value="libcommon_cv.so"
            file_type="shared"
            install_softlink="$(TARGET_ENV)/lib64/libcommon_cv.so"/>
    </file_info>
</block>
